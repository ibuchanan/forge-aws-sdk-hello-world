{
  "name": "forge-aws-demo",
  "version": "0.0.0",
  "description": "Example Forge app using native Node.js runtime and AWS SDK for JavaScript",
  "private": true,
  "license": "Apache-2.0",
  "homepage": "https://github.com/ibuchanan/forge-aws-sdk-hello-world",
  "contributors": [
    "Ian Buchanan <ibuchanan@atlassian.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ibuchanan/forge-aws-sdk-hello-world"
  },
  "engines": {
    "npm": "9",
    "node": "18"
  },
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "update": "npm -s run update:node",
    "update:node": "./scripts/set-node-version.sh",
    "clean": "rm -rf ./out",
    "build": "npm -s run clean && npm -s run build:typescript",
    "build:typescript": "tsc",
    "forge:deploy": "forge deploy --no-verify",
    "forge:environments": "forge environments",
    "forge:feedback": "forge feedback",
    "forge:install": "forge install",
    "forge:install-list": "forge install list",
    "forge:install:upgrade": "forge install --upgrade",
    "forge:logs": "forge logs",
    "forge:login": "forge login",
    "forge:logout": "forge logout",
    "forge:providers": "forge providers",
    "forge:register": "forge register",
    "forge:settings": "forge settings",
    "forge:storage": "forge storage",
    "forge:tunnel": "forge tunnel",
    "forge:uninstall": "forge uninstall",
    "forge:variables": "forge variables",
    "forge:webtrigger": "forge webtrigger",
    "forge:whoami": "forge whoami",
    "lint": "npm -s run lint:typescript && npm -s run lint:dotenv && npm -s run lint:eslint && npm -s run lint:prettier && npm -s run lint:forge",
    "lint:fix": "npm -s run lint:typescript && npm -s run lint:dotenv && npm -s run lint:fix:eslint && npm -s run lint:fix:prettier && npm -s run lint:fix:forge",
    "lint:typescript": "npm -s run build:typescript -- --noEmit",
    "lint:dotenv": "./scripts/check-env.js",
    "lint:eslint": "eslint .",
    "lint:fix:eslint": "npm -s run lint:eslint -- --fix",
    "lint:prettier": "prettier .",
    "lint:fix:prettier": "npm -s run lint:prettier -- --write",
    "lint:forge": "forge lint",
    "lint:fix:forge": "npm -s run lint:forge -- --fix",
    "lint:forge:lint:verbose": "forge lint --verbose",
    "test": "npm -s run test:jest",
    "test:jest": "jest .",
    "provision": "echo not-implemented"
  },
  "devDependencies": {
    "@forge/cli": "^6.21",
    "@tsconfig/node18": "18",
    "@types/jest": "24",
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "6",
    "@typescript-eslint/parser": "6",
    "eslint": "8",
    "eslint-config-prettier": "9",
    "eslint-import-resolver-typescript": "3",
    "eslint-plugin-import": "2",
    "eslint-plugin-jest": "27",
    "eslint-plugin-prettier": "5",
    "jest": "29",
    "jest-junit": "16",
    "prettier": "3",
    "ts-node": "10",
    "typescript": "5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3",
    "dotenv-safe": "^8",
    "zod": "^3"
  }
}
