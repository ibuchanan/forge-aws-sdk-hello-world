{
  "name": "forge-aws-sdk-hello-world",
  "version": "0.0.0",
  "description": "Example Forge app using AWS SDK with Forge's native Node.js runtime",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ibuchanan/forge-aws-sdk-hello-world",
  "contributors": [
    "Ian Buchanan <ibuchanan@atlassian.com>"
  ],
  "repository": {
    "type": "git",
