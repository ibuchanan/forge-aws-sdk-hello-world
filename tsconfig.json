{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "declaration": false,
    "esModuleInterop": true,
    "importHelpers": false,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "outDir": "./out",
    "rootDir": "src",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2017",
    "types": ["jest", "node"],
    "jsx": "react-jsx"
  }
}
